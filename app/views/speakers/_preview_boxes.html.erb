<div class="container preview-speakers">
	<div class="row">
		<% if speakers.empty? and params[:search].present? and params[:search][:q].present? %>
			<div class="col-xs-12 text-center speakers-empty">
				<h4>No se encontraron resultados para: <em><%= params[:search][:q] %></em></h4>
			</div>
		<% end %>
		<%= render speakers %>
	</div>
</div>