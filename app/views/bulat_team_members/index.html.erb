<p id="notice"><%= notice %></p>

<h1>Listing Bulat Team Members</h1>

<table width="100%">
  <thead>
    <tr>
      <th>Title</th>
      <th>Link</th>
      <th>Color</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @bulat_team_members.each do |bulat_team_member| %>
      <tr>
        <td><%= bulat_team_member.title %></td>
        <td><%= bulat_team_member.link %></td>
        <td><%= bulat_team_member.color %></td>
        <td><%= link_to 'Show', bulat_team_member %></td>
        <td><%= link_to 'Edit', edit_bulat_team_member_path(bulat_team_member) %></td>
        <td><%= link_to 'Destroy', bulat_team_member, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Bulat team member', new_bulat_team_member_path %>
